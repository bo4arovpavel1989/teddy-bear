<form id="changePage"  action="/admin/changepage" method="post" enctype="multipart/form-data" style="margin-left: 5%;">
    <div class="form-group">
		<div class="input-group">
			<input type="login" class="form-control" name='page' value='{{page}}' required readonly>
		</div>
	</div>
	<div class="form-group">
		<div class="input-group">
			<textarea rows="45" cols="150" name="content" >{{content}}</textarea>
		</div>
	</div>
    <div class="form-group">
		<input class="btn btn-primary" type="submit" value="Изменить"></input>
		<a href='#' class='btn btn-default' onclick='$(".changePageFormPlace").empty(); return false;'>отмена</a>
 </div>
</form>